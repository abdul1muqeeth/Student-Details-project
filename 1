class Student:
    """ i am createing one project in this project i am taking details of student"""
    def __init__(self,name,branch,year,roll_no,backlock):
        self.Name=name
        self.Branch=branch
        self.Year=year
        self.roll_no=roll_no
        self.backlock=backlock
    def info(self):
        print("Name of the Student is : "+self.Name)
        print("Branch of the Student is : "+self.Branch)
        print("Year of the Student is : "+self.Year)
        print("Roll Number of the Student is : "+self.roll_no)
        print("How backlock you have : "+self.backlock)
list_storedata=[]
while True:
    name=input("Enter the Name of the Student\t")
    branch=input("Enter  the Branch of the Student\t")
    year=input("Enter the Year of the Student\t")
    roll_no=input("Enter the Roll Number of the Student\t")
    backlock=input("Enter the Backlock you have\t")
    m=Student(name,branch,year,roll_no,backlock)
    list_storedata.append(m)
    b=input("You want to Enter the details again[yes/no]")
    if b.lower()=='no':
        break
print("Sucefully uploaded")
for n in list_storedata:
    n.info()
    print("")
